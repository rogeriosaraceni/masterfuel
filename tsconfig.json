{
    "extends": "astro/tsconfigs/base",

    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": ["src/assets/*"],
            "@data/*": ["src/data/*"],
            "@layouts/*": ["src/layouts/*"],
            "@components/*": ["src/components/*"],
            "@styles/*": ["src/styles/*"],
            "@utils/*": ["src/utils/*"]
        }
    }
}
